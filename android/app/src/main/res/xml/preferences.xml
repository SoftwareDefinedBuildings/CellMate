<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <EditTextPreference
        android:defaultValue="@string/cellmate_server_addr_val"
        android:key="@string/cellmate_server_addr_key"
        android:title="@string/cellmate_server_addr_title"/>
    <EditTextPreference
        android:defaultValue="@string/cellmate_server_port_val"
        android:key="@string/cellmate_server_port_key"
        android:numeric="integer"
        android:title="@string/cellmate_server_port_title"/>
    <EditTextPreference
        android:defaultValue="@string/bosswave_router_addr_val"
        android:key="@string/bosswave_router_addr_key"
        android:title="@string/bosswave_router_addr_title"/>
    <EditTextPreference
        android:defaultValue="@string/bosswave_router_port_val"
        android:key="@string/bosswave_router_port_key"
        android:numeric="integer"
        android:title="@string/bosswave_router_port_title"/>
    <EditTextPreference
        android:defaultValue="@string/bosswave_key_base64_val"
        android:key="@string/bosswave_key_base64_key"
        android:title="@string/bosswave_key_base64_title"/>
    <EditTextPreference
        android:defaultValue="@string/resolution_val"
        android:enabled="false"
        android:key="@string/resolution_key"
        android:title="@string/resolution_title"/>
    <EditTextPreference
        android:defaultValue="@string/fx_val"
        android:key="@string/fx_key"
        android:numeric="decimal"
        android:title="@string/fx_title"/>
    <EditTextPreference
        android:defaultValue="@string/fy_val"
        android:key="@string/fy_key"
        android:numeric="decimal"
        android:title="@string/fy_title"/>
    <EditTextPreference
        android:defaultValue="@string/cx_val"
        android:key="@string/cx_key"
        android:numeric="decimal"
        android:title="@string/cx_title"/>
    <EditTextPreference
        android:defaultValue="@string/cy_val"
        android:key="@string/cy_key"
        android:numeric="decimal"
        android:title="@string/cy_title"/>
    <CheckBoxPreference
        android:defaultValue="true"
        android:key="@string/flip_x_key"
        android:summary="@string/flip_x_summary"
        android:title="@string/flip_x_title"/>
</PreferenceScreen>